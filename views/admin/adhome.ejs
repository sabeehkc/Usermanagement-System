
<input type="search">



<table width="50%">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Verified</th>
    </tr>

    <% 
        if(users.length > 0){
            for(let i=0;i<users.length;i++){
                %>
                <tr>
                    <td><%= users[i].name %> </td>
                    <td><%= users[i].email %> </td>
                    <td><%= users[i].mobile %> </td>
                    <td><%
                        if(users[i].is_verified == 0){
                            %>
                             <p>Not Verified</p>   
                            <%
                        }else{
                            %>
                            <p>Verified</p>
                            <% 
                        }
                        %> </td>
                </tr>
                <%
            }
            
        }else{
            %>
            <tr>
                <td colspan="S">Users Not Found</td>
            </tr>
            <%
        }
    %>


</table>
<br>



<a href="/admin/logout">Logout</a>